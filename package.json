{
  "name": "mtproto-core",
  "version": "6.1.4",
  "type": "module",
  "description": "Telegram API JS (MTProto) client library for Node.js and browser",
  "repository": "github:fedyk/mtproto-core",
  "main": "./index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "node --test",
    "generate-builder": "node scripts/generate-builder.js",
    "generate-parser": "node scripts/generate-parser.js",
    "generate-tl": "run-p generate-builder generate-parser",
    "prepublishOnly": "run-s generate-tl"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "aes-js": "3.1.2",
    "big-integer": "1.6.51",
    "debug": "4.3.3",
    "events": "3.3.0",
    "leemon": "6.2.0",
    "lodash.debounce": "4.0.8",
    "pako": "2.0.4"
  },
  "devDependencies": {
    "@types/aes-js": "^3.1.1",
    "@types/debug": "^4.1.8",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^20.4.5",
    "@types/pako": "^2.0.0",
    "jest": "27.4.5",
    "npm-run-all": "4.1.5",
    "typescript": "^5.1.6"
  }
}
